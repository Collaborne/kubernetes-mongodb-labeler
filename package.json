  "engines": {
    "node": ">=8.9.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^5.0.0",
    "mocha": "^6.0.0",
    "nyc": "^14.0.0"
  },
  "dependencies": {
    "check-node-version": "^4.0.0",
